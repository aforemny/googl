#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {   
        int n,c,i,j;
        cin>>n;
        int a[10000],b[10000];
        for(int i=0;i<n;i++)
        {
		cin>>a[i];
        b[i]={0};
    }
        
        for(i=0;i<n-1;i++)
        {
        	c=0;
        	for(j=0;j<n;j++)
        	{
        		if(a[i]<a[j])
        		{
        			c++;
				}
			}
			b[i]=c;
		}
        for( i=0;i<n;i++)
        cout<<b[i]<<" ";
        cout<<endl;
    }
    
}
